{
	"name": "slightly-better-gh-search",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:supabase": "deno test --allow-net --allow-env --env-file=supabase/.env.test --config=supabase/functions/deno.json supabase/functions/**/*_test.ts",
		"test:supabase:serve": "supabase functions serve --env-file supabase/.env.test",
		"test:supabase:watch": "deno test --allow-net --allow-env --env-file=supabase/.env.test --config=supabase/functions/deno.json --watch supabase/functions/**/*_test.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.2",
		"@iconify/json": "^2.2.426",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.3",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^20.19.27",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^16.5.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"supabase": "^2.76.6",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.52.0",
		"unplugin-icons": "^22.5.0",
		"vite": "^7.3.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"supabase"
		],
		"overrides": {
			"cookie": "^0.7.0"
		}
	},
	"dependencies": {
		"@supabase/ssr": "^0.8.0",
		"@supabase/supabase-js": "^2.90.1"
	}
}